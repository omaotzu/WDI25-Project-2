<div class="container">
  <form method="POST" action="/places/<%= place.id %>">
    <div>
      <label for="name">Name</label>
      <input type="text" name="name" id="name" value="<%= place.name %>">
    </div>
    <div>
      <label for="region">Region</label>
      <input type="text" name="region" id="region" value="<%= place.region %>">
    </div>
    <div>
      <label for="licence">Licence required?</label>
      <input type="text" name="licence" id="licence" value="<%= place.licence %>">
    </div>
    <div>
      <label for="rating">Rating</label>
      <input type="number" name="rating" id="rating" value="<%= place.rating %>">
    </div>
    <div>
      <label for="keywords">Keywords</label>
      <input type="text" name="keywords" id="keywords" value="<%= place.keywords %>">
    </div>
    <div>
      <label for="price">Price (if applicable)</label>
      <input type="number" name="price" id="price" value="<%= place.price %>">
    </div>
    <div>
      <label for="address">Address</label>
      <input type="text" name="address" id="address" value="<%= place.address %>">
    </div>
    <div>
      <label for="description">Description</label>
      <input type="textarea" name="description" id="description" value="<%= place.description %>">
    </div>
    <button class="btn btn-success">Add Location!</button>
  </form>
</div>
